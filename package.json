	},

	"bugs": {
		"url": "https://github.com/vanruesc/math-ds/issues"
	},

	"files": [
		"src",
		"build"
	],

	"scripts": {
		"ava": "ava",
		"lint": "npm run lint:lib && npm run lint:test",
		"lint:config": "eslint rollup.config.js",
		"lint:lib": "eslint src",
		"lint:test": "eslint test",
		"build": "rollup -c",
